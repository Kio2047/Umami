{
  "compilerOptions": {
    // "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "module": "commonjs",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "target": "ES2022",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist/",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    },
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["./src/services/DBSeeder/seedDB.ts"]
}
