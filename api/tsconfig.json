{
  "compilerOptions": {
    // "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "target": "ES2022",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types"]
    // "paths": {
    //   "#src/*": ["src/*"]
    // },
  },
  "include": ["src/**/*"]
  // "exclude": ["src/types/environment.d.ts"]
}
